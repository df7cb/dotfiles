screen "0" {
  general {
    border = 1
    snap = 8
    resize_hints = true
    opacity_unfocused = 100
    focus_move_pointer = false
    allow_lower_floats = false
    sloppy_focus = true
    new_become_master = true
    new_get_focus = true
    font = "mono-12"
  }
  statusbar "mystatusbar" {
    position = "top"
    height = 0
    width = 0
    taglist "mytaglist" {
      mouse {
        modkey = {}
        button = "1"
        command = "tag_view"
      }
      mouse {
        modkey = {"Mod4"}
        button = "1"
        command = "client_tag"
      }
      mouse {
        modkey = {}
        button = "3"
        command = "tag_toggleview"
      }
      mouse {
        modkey = {"Mod4"}
        button = "3"
        command = "client_toggletag"
      }
      mouse {
        modkey = {}
        button = "4"
        command = "tag_viewnext"
      }
      mouse {
        modkey = {}
        button = "5"
        command = "tag_viewprev"
      }
    }
    layoutinfo "mylayoutinfo" {
      mouse {
        modkey = {}
        button = "1"
        command = "tag_setlayout"
        arg = "+1"
      }
      mouse {
        modkey = {}
        button = "4"
        command = "tag_setlayout"
        arg = "+1"
      }
      mouse {
        modkey = {}
        button = "3"
        command = "tag_setlayout"
        arg = "-1"
      }
      mouse {
        modkey = {}
        button = "5"
        command = "tag_setlayout"
        arg = "-1"
      }
    }
    tasklist "mytasklist" {
      mouse {
        modkey = {}
        button = "4"
        command = "client_focusnext"
        # arg = ""
      }
      mouse {
        modkey = {}
        button = "5"
        command = "client_focusprev"
        # arg = ""
      }
      mouse {
        modkey = {"Mod4"}
        button = "4"
        command = "client_swapnext"
        # arg = ""
      }
      mouse {
        modkey = {"Mod4"}
        button = "5"
        command = "client_swapprev"
        # arg = ""
      }
      # fg = ""
      # bg = ""
      # focus_fg = ""
      # focus_bg = ""
      # font = ""
      align = "left"
      show_icons = true
      show_all = false
    }
    textbox clock {
      text = "                          "
      bg = "#3333ff"
    }
  }
  tags {
    tag "eins" {
      layout = "tile"
      mwfact = 0.500000
      nmaster = 1
      ncol = 1
    }
    tag "zwei" {
      layout = "tile"
      mwfact = 0.500000
      nmaster = 1
      ncol = 1
    }
    tag "drei" {
      layout = "tile"
      mwfact = 0.500000
      nmaster = 1
      ncol = 1
    }
    tag "vier" {
      layout = "tile"
      mwfact = 0.500000
      nmaster = 1
      ncol = 1
    }
    tag "f√ºnf" {
      layout = "tile"
      mwfact = 0.500000
      nmaster = 1
      ncol = 1
    }
  }
  colors {
    normal_border = "#111111"
    normal_bg = "#111111"
    normal_fg = "#ffffff"
    focus_border = "#ecb204"
    focus_bg = "#9cd234"
    focus_fg = "#ffffff"
    urgent_bg = "#ff0000"
    urgent_fg = "#ffffff"
    tab_border = "#ff0000"
  }
  layouts {
    layout "tile" {
      image = "/usr/share/awesome/icons/layouts/tilew.png"
    }
    layout "tileleft" {
      image = "/usr/share/awesome/icons/layouts/tileleftw.png"
    }
    layout "spiral" {
      image = "/usr/share/awesome/icons/layouts/spiralw.png"
    }
    layout "dwindle" {
      image = "/usr/share/awesome/icons/layouts/dwindlew.png"
    }
    layout "max" {
      image = "/usr/share/awesome/icons/layouts/maxw.png"
    }
    layout "floating" {
      image = "/usr/share/awesome/icons/layouts/floatingw.png"
    }
  }
  padding {
    top = 0
    bottom = 0
    right = 0
    left = 0
  }
}
