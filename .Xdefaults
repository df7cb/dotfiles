! https://unix.stackexchange.com/questions/118641/font-rendering-issue-in-urxvt-too-much-space-between-characters
URxvt.letterSpace:	-1
URxvt.internalBorder:	0
URxvt.iso14755:		false
URxvt.keysym.M-C-1:	command:\033]710;5x8\007
URxvt.keysym.M-C-2:	command:\033]710;6x10\007
URxvt.keysym.M-C-3:	command:\033]710;xft:DejaVu Sans Mono-8\007
URxvt.keysym.M-C-4:	command:\033]710;xft:DejaVu Sans Mono-12\007
URxvt.keysym.M-C-5:	command:\033]710;xft:DejaVu Sans Mono-15\007
URxvt.keysym.M-C-6:	command:\033]710;xft:DejaVu Sans Mono-19\007
URxvt.keysym.M-C-7:	command:\033]710;xft:DejaVu Sans Mono-23\007
URxvt.keysym.M-C-8:	command:\033]710;xft:DejaVu Sans Mono-28\007
URxvt.keysym.M-C-9:	command:\033]710;xft:DejaVu Sans Mono-35\007
! for whatever reason, there is no XK_spade in keysymdef.h, pick something else and remap that here
URxvt.keysym.maltesecross: \342\231\240
URxvt.keysym.C-equal:	font-size:increase
URxvt.keysym.C-slash:	font-size:show
URxvt.matcher.button:	3
URxvt.matcher.pattern.1: (?:Closes:.|\\#)([2-9]\\d{5}\\b|1\\d{6}\\b)
URxvt.matcher.launcher.1: x-www-browser https:\/\/bugs.debian.org/cgi-bin/bugreport.cgi?bug=$1
URxvt.matcher.pattern.2: (\\d{6,7})@bugs.debian.org
URxvt.matcher.launcher.2: x-www-browser https:\/\/bugs.debian.org/cgi-bin/bugreport.cgi?bug=$1
URxvt.matcher.pattern.3: <([^<>]+[0-9][^<>]+@[^<>]+)>
URxvt.matcher.launcher.3: x-www-browser https:\/\/lists.debian.org/$1
URxvt.matcher.pattern.4: (\\b[\\w-]+)\\([1-9]\\)
URxvt.matcher.launcher.4: x-www-browser https:\/\/manpages.debian.org/$1
URxvt.matcher.pattern.5: rt.debian.org.\\#(\\d{4,})
URxvt.matcher.launcher.5: x-www-browser https:\/\/rt.debian.org/Ticket/Display.html?id=$1
URxvt.matcher.pattern.6: \\#(1\\d{5}\\b)
URxvt.matcher.launcher.6: x-www-browser https:\/\/rt.credativ.com/Ticket/Display.html?id=$1
URxvt.matcher.pattern.7: \\b([A-Ra-r][A-Ra-r][0-9][0-9]([A-Xa-x][A-Xa-x]([0-9][0-9]([A-Xa-x][A-Xa-x])?)?)?)\\b
URxvt.matcher.launcher.7: x-www-browser https:\/\/k7fry.com/grid/?qth=$1
URxvt.mouseWheelScrollPage: true
URxvt.perl-ext:		default,font-size,tabbed,matcher,vtwheel,3digits,-confirm-paste
URxvt.print-pipe:	cat > $(TMPDIR=$HOME mktemp urxvt.XXXXXX)
URxvt.saveLines:	5000
URxvt.scrollBar_floating: true
URxvt.scrollColor:	#858585
URxvt.scrollstyle:	plain
URxvt.scrollTtyKeypress: true
URxvt.scrollTtyOutput:	false
URxvt.scrollWithBuffer:	true
URxvt.selection.pattern-0: ([^:„“”«»]+)
URxvt.underlineColor:	#888888

