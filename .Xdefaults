!xterm*VT100.Translations: #override <Key>BackSpace: string(0x08) \n
xterm*VT100.Translations: #override \n\
	Ctrl ~Shift <KeyPress> -: smaller-vt-font() \n\
	Ctrl ~Shift <KeyPress> =: larger-vt-font() \n\
	Shift <KeyPress> Prior:scroll-back(2,halfpage) \n\
	Shift <KeyPress> Next:scroll-forw(2,halfpage) \n\
	<Btn4Down>:scroll-back(2,halfpage,m)     \n\
	<Btn5Down>:scroll-forw(2,halfpage,m)     \n\
	Ctrl Shift <KeyPress> p: string(0xe2) string(0x99) string(0xa0)
